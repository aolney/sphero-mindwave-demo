    "cylon-sphero": "github:aolney/cylon-sphero#1.0.0",
    "downshift": "^3.4.1",
    "match-sorter": "^4.0",
    "mdi-material-ui": "^6.8.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "serialport": "^8.0.5",
